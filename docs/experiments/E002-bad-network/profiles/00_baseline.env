USBIP_SERVER_IP=192.168.1.149
USBIP_SERVER_PORT=3240

MOUNTPOINT=/mnt/usb

# netem (baseline)
DELAY=0ms
JITTER=0ms
LOSS=0%
LIMIT=1000

# fio knobs (safe defaults to avoid E003 hang)
TESTFILE=/mnt/usb/fio_testfile.bin
SIZE=256m
RUNTIME=15
IODEPTH=1
NUMJOBS=1

# per-job timeout (seconds)
FIO_TIMEOUT=120
